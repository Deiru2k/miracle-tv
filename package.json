{
  "name": "miracle-tv",
  "version": "0.1.0.3",
  "main": "server.js",
  "repository": "https://code.gensokyo.social/Gensokyo.social/miracle-tv",
  "license": "MIT",
  "scripts": {
    "daemon:start": "NODE_ENV=production name=miracle-tv pm2 start yarn --name miracle-tv -- server",
    "daemon:stop": "pm2 delete miracle-tv",
    "daemon:restart": "name=miracle-tv pm2 restart miracle-tv --update-env",
    "server": "NODE_ENV=production $node_modules/.bin/ts-node -r tsconfig-paths/register src/server.ts",
    "codegen": "graphql-codegen --config codegen.yml",
    "dev": "nodemon -e ts,tsx,graphql --exec \"yarn run server\""
  },
  "dependencies": {
    "apollo-server": "^2.24.0",
    "apollo-server-express": "^2.24.0",
    "async": "^3.2.0",
    "express": "^4.17.1",
    "express-graphql": "^0.12.0",
    "glob": "^7.1.7",
    "graphql": "^15.5.0",
    "luxon": "^1.27.0",
    "md5": "^2.3.0",
    "module-alias": "^2.2.2",
    "pm2": "^4.5.6",
    "ramda": "^0.27.1",
    "rethinkdb": "^2.4.2",
    "tsconfig-paths": "^3.9.0"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "1.21.4",
    "@graphql-codegen/introspection": "1.18.2",
    "@graphql-codegen/typescript": "1.22.0",
    "@graphql-codegen/typescript-resolvers": "1.19.1",
    "@types/async": "^3.2.6",
    "@types/express": "^4.17.11",
    "@types/express-graphql": "^0.9.0",
    "@types/glob": "^7.1.3",
    "@types/luxon": "^1.26.5",
    "@types/md5": "^2.3.0",
    "@types/module-alias": "^2.0.0",
    "@types/ramda": "^0.27.40",
    "@types/rethinkdb": "^2.3.16",
    "@vercel/ncc": "^0.28.6",
    "file-loader": "^6.2.0",
    "json-loader": "^0.5.7",
    "nodemon": "^2.0.7",
    "prettier": "^2.3.0",
    "ts-loader": "^9.2.1",
    "ts-node": "^9.1.1",
    "typescript": "^4.2.4",
    "webpack": "^5.37.1",
    "webpack-cli": "^4.7.0"
  }
}
